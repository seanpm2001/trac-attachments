+/////////////////////////////////////////////////////////////////////////////
+// Name:        wave.h
+// Purpose:     Plays wave files using wxAudio
+// Author:      Ryan Norton
+// Modified by:
+// RCS-ID:      $$
+// Copyright:   (c) Ryan Norton, Julian Smart
+// Licence:     wxWindows license
+/////////////////////////////////////////////////////////////////////////////
+
+//
+//  Header for WAVE files.
+//
+typedef struct
+{
+	unsigned char		riff[4];		// 'RIFF'
+	unsigned int	riffSize;
+	unsigned char		wave[4];		// 'WAVE'
+	unsigned char		fmt[4];			// 'fmt '
+	unsigned		fmtSize;
+	unsigned short	Format;                              
+	unsigned short	Channels;
+	unsigned		SamplesPerSec;
+	unsigned		BytesPerSec;
+	unsigned short	BlockAlign;
+	unsigned short	BitsPerSample;
+	unsigned char		data[4];		// 'data'
+	unsigned		dataSize;
+} wxWaveStruct;
+
+/**
+*   Plays WAVE files on any OS.  Does not support compressed WAVE files (yet).
+*/
+class wxWave : public wxAudioFile
+{
+////////////////////////////////////////////////////////////////////
+                            protected:
+////////////////////////////////////////////////////////////////////
+    /** 
+    *   Contains data related to the wave file
+    */
+    wxWaveStruct kWS;
+
+////////////////////////////////////////////////////////////////////
+                            public:
+////////////////////////////////////////////////////////////////////
+    /**
+    *   Initilizes the audio file
+    */
+    wxWave();
+
+    /**
+    *   Initilizes and opens the audio file
+    */
+    wxWave(const char* szFile, OpenMode mode = wxFile::read); 
+
+    /**
+    *   Closes and cleans up resources related to the file
+    */
+    virtual ~wxWave();
+
+    /**
+    *   Closes the file.
+    */
+    virtual int Close();
+
+    /**
+    *   Opens the file from a location on disk
+    *
+    *   Parameters identical to wxFile.
+    *
+    *   @param szFile file to open (full or relative path)
+    *   @param mode Mode to open the file in.  Only open with wxFile::read and wxFile::read_write
+    */
+    virtual bool Open(const char* szFile, OpenMode mode = wxFile::read);
+    
+    /**
+    *   Reads data converted to PCM.  
+    *
+    *   @param nNumToRead number of UNCOMPRESSED bytes to read
+    *   @return number of UNCOMPRESSED bytes read
+    */
+    virtual off_t ReadPCM (void* pData, off_t nNumToRead);
+
+    /**
+    *   Seeks to a current time in the file
+    */
+    virtual double SeekTime(double dWhere);
+    
+//
+//
+//  Functions for backwards compatability (REMOVE?)
+//
+//
+    bool Create(const char* szFile, bool bLoadFromResource = false);
+    bool IsOk() const;                                
+};//end of wxWave
