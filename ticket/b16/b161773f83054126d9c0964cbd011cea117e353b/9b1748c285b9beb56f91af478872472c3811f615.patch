diff -ru2 -N -xCVS -x.#* -x*.orig -x*.rej -x*~ wxWindows.orig\include\wx\audio.h wxWindows\include\wx\audio.h
--- wxWindows.orig\include\wx\audio.h	Wed Dec 31 16:00:00 1969
+++ wxWindows\include\wx\audio.h	Mon Apr 28 14:00:23 2003
@@ -0,0 +1,225 @@
+/////////////////////////////////////////////////////////////////////////////
+// Name:        audio.h
+// Purpose:     Audio API Proposal
+// Author:      Ryan Norton
+// Modified by:
+// RCS-ID:      $$
+// Copyright:   (c) Ryan Norton
+// Licence:     wxWindows license
+/////////////////////////////////////////////////////////////////////////////
+
+//
+//  Notes about coding practices:
+//  If I make a parameter to a function -1 by default,
+//  then it will be initialized to the max.  Ex:
+//  Play(double dStart = 0, double dEnd = -1) where dEnd
+//  intiatially is the end of the file
+//
+
+#ifndef _WX_AUDIOH__
+#define _WX_AUDIOH__
+
+#if defined(__GNUG__) && !defined(__APPLE__)
+#pragma interface "audio.h"
+#endif
+
+//defs
+#define WX_FORMAT_MONO8 AL_FORMAT_MONO8
+#define WX_FORMAT_MONO16 AL_FORMAT_MONO16
+#define WX_FORMAT_STEREO8 AL_FORMAT_STEREO8
+#define WX_FORMAT_STEREO16 AL_FORMAT_STEREO16
+
+#define NUMBUFFERS 20
+
+//typedefs 
+typedef unsigned int WXAUDIOSOURCE;
+typedef unsigned int WXAUDIOBUFFER;
+
+//wx headers required:
+#include <wx/file.h>
+
+//outside headers
+#include <alut.h>           //note that all openal-specific stuff is in audio.cpp
+#include <vorbis/vorbisfile.h>
+#include <ogg/ogg.h>
+
+//inside(wx) headers
+#include "generic/audiofile.h"
+#include "ogg/ogg.h"
+#include "generic/wave.h"
+
+//required libraries
+#ifdef _MSC_VER 
+    #pragma comment(lib, "openal32.lib")
+    #pragma comment(lib, "alut.lib")
+    #pragma comment(lib, "ogg_static.lib")
+    #pragma comment(lib, "vorbisfile_static.lib")
+    #pragma comment(lib, "vorbis_static.lib")
+#endif
+
+#define wxOPENAL_CHECK(cmd)         \
+cmd;\
+{int nError; if(                                                                        \
+    (nError = alGetError()) != AL_NO_ERROR){                                 \
+                                                                                  \
+            wxLogSysError(wxString::Format("[wxAudio Command:] %s [Failed!]\n[Error Message:] %s",    \
+                                #cmd,                                                   \
+                                alGetString(nError)                 \
+                            ));                                          \
+wxFAIL_MSG("OPENAL ERROR!"); return false;}}                                                      
+
+#define wxOPENAL_CHECK2(cmd)         \
+cmd;\
+{int nError; if(                                                                        \
+    (nError = alGetError()) != AL_NO_ERROR){                                 \
+                                                                                  \
+            wxMessageBox(wxString::Format("[wxAudio Command:] %s [Failed!]\n[Error Message:] %s",    \
+                                #cmd,                                                   \
+                                alGetString(nError)                 \
+                            ));                                           \
+}}                                                      
+
+#define wxCHECK_LOG(arg, mes) \
+    if (!(arg)) \
+        wxLogSysError(mes);
+/**
+*	Master class that initializes the audio subsystem.
+*/
+class wxAudio
+{
+////////////////////////////////////////////////////////////////////
+                            protected:
+////////////////////////////////////////////////////////////////////
+    /**
+    *   If Init() has been called & called successfully
+    */
+    static bool bInit;
+    
+    /**
+    *   If EAX is present or not
+    */
+    static int nEAXLevel;
+
+////////////////////////////////////////////////////////////////////
+                            public:
+////////////////////////////////////////////////////////////////////
+    /**
+    *   not implemented (or meant to)
+    */
+    wxAudio();
+
+    /**
+    *   not implemented (or meant to)
+    */
+    ~wxAudio();
+
+    /**
+    *   Creates all nessary buffers and sources required for playback.
+    *
+    *
+    *   @param nBuffer [OUT] Variable to recieve ID of buffer created.
+    *   @param nSource [OUT] Variable to recieve ID of source created.
+    *   @param pData  Data of the source (i.e. actual bytes of a WAV file)
+    *   @param nFormat Format of the audio - 
+    *   FORMAT_MONO8, FORMAT_MONO16, FORMAT_STEREO8, FORMAT_STEREO16
+    *   ex.  FORMAT_MONO8 means 8 bit mono sound.
+    *   @param nFreq Frequency of audio.  Typical value is 44100.
+    *   @param nSize Size of pData.
+    *
+    static bool GenerateSource( WXAUDIOBUFFER* nBuffer, WXAUDIOSOURCE* nSource,
+                                void* pData, int nFormat, int nFreq, 
+                                int nSize);
+
+    /**
+    *   Obtains the presence of EAX
+    *
+    *   @return An integer representing the version of EAX
+    *   present of the machine - i.e. 3 is version 3.0.
+    *   A Version of 0 means EAX does not exist.
+    */
+    static int GetEAXLevel();
+
+    /**
+    *   Initializes the audio driver.
+    *
+    *   Also attempts to detect any presence of the EAX extensions 1-3.
+    */
+    static bool Init();
+
+    /**
+    *   Returns true if the audio system is initialized correctly
+    */
+    static bool IsInitialized();
+
+    /**
+    *   Determines if a file is playing or not
+    */
+    static bool IsFilePlaying(wxAudioFile& pFile);
+    
+    /**
+    *   Opens the said file on disk.
+    *
+    *   Call delete when done with the file.
+    */
+    static wxAudioFile* OpenFile(const char* szFile);
+
+    /**
+    *   Plays the specified file.
+    */
+    static bool PlayFile (wxAudioFile& pFile);
+    
+    /**
+    *   Sets the position of the listener (the "player" or user)
+    * determines how loud a sound is when played.
+    *
+    *   @param dXPos X position of listener
+    *   @param dYPos Y position of listener
+    *   @param dZPos Z position of listener
+    *   @param dXAt X velocity of At Vector
+    *   @param dYAt Y velocity of At Vector
+    *   @param dZAt Z velocity of At Vector
+    *   @param dXUp X velocity of Up(Y) Vector
+    *   @param dYUp Y velocity of Up(Y) Vector
+    *   @param dZUp Z velocity of Up(Y) Vector
+    */
+    static bool SetListener(
+                     float dXPos = 0.0, float dYPos = 0.0, float dZPos = 0.0,
+                     float dXAt = 0.0, float dYAt = 0.0, float dZAt = -1.0,
+                     float dXUp = 0.0, float dYUp = 1.0, float dZUp = 0.0);
+
+    /**
+    *   Closes the audio driver.
+    */
+    static void Shutdown();
+
+    /**
+    *   Stops playing or streaming the file
+    */
+    static void StopFile (wxAudioFile& pFile);
+
+    /**
+    *   Streams the file.
+    */
+    static bool StreamFile (wxAudioFile& pFile);
+};//end of wxAudio
+
+/**
+*   Helper class for starting up and shutting down wxAudio
+*/
+class wxAudioApp : wxApp
+{
+////////////////////////////////////////////////////////////////////
+                            public:
+////////////////////////////////////////////////////////////////////
+    /**
+    *   Initilizes wxAudio.
+    */
+    virtual bool OnInit();
+
+    /**
+    *   Closes wxAudio.
+    */
+    virtual int OnExit();
+};//end of wxAudioApp
+
+#endif // _WX_AUDIOH__
diff -ru2 -N -xCVS -x.#* -x*.orig -x*.rej -x*~ wxWindows.orig\include\wx\generic\audiofile.h wxWindows\include\wx\generic\audiofile.h
--- wxWindows.orig\include\wx\generic\audiofile.h	Wed Dec 31 16:00:00 1969
+++ wxWindows\include\wx\generic\audiofile.h	Mon Apr 28 15:49:12 2003
@@ -0,0 +1,249 @@
+/////////////////////////////////////////////////////////////////////////////
+// Name:        audiofile.h
+// Purpose:     Parent class of audiofiles(WAV, OGG) in wxAudio
+// Author:      Ryan Norton
+// Modified by:
+// RCS-ID:      $$
+// Copyright:   (c) Ryan Norton
+// Licence:     wxWindows license
+/////////////////////////////////////////////////////////////////////////////                                                     
+
+typedef struct
+{
+    double dCurrentPos,  //how many seconds into the file playing is
+           dEndPos,           //where to end when playing file
+           dStartPos;         //where to start in the file
+    int    
+          nFormat;              //format of file.  Implementation specific.
+    long      nRate;                //for wave files usually 44100
+
+    bool bLoop;             //looping?
+
+    double dPlayingTime;    //Total playing time of the file (in seconds)
+
+    off_t nPCMLength;       //uncompressed size of file data
+
+    off_t nBytesPerSecPCM;
+}wxAudioFileInfo;
+/**
+*   parent class of all audio files.
+*
+*   Note that all times are in SECONDS, so if the variable is a double,
+*   then 2.34 seconds means 2 & 1/3 seconds.
+*/
+class wxAudioFile : public wxFile,      //for file i/o
+                    public wxThread     //for streaming
+{
+////////////////////////////////////////////////////////////////////
+                            protected:
+////////////////////////////////////////////////////////////////////    
+    
+    /**
+    *   Whether to stream the file in the thread or not
+    */
+    bool bStream;
+
+    /**
+    *   Internal info about file.
+    */
+    wxAudioFileInfo kInfo;
+    
+    /**
+    *   Source the thread is currently using.
+    */
+    WXAUDIOSOURCE kSource;
+
+    /**
+    *   Buffers the thread is currently using.
+    */
+    WXAUDIOBUFFER* kBuffers;
+
+    /**
+    *   Called when wanting to stream file in a seperate thread
+    */
+    virtual ExitCode Entry();
+
+////////////////////////////////////////////////////////////////////
+                            public:
+////////////////////////////////////////////////////////////////////
+    /**
+    *   Initilizes the audio file
+    */
+    wxAudioFile();
+
+    /**
+    *   Initilizes and opens the audio file
+    */
+    wxAudioFile(const char* szFile, OpenMode mode = wxFile::read); 
+
+    /**
+    *   Closes and cleans up resources related to the file
+    */
+    virtual ~wxAudioFile();
+
+    //
+    //  PURE VIRTUAL FUNCTIONS
+    //  
+
+    /**
+    *   Closes the file.
+    */
+    virtual int Close();// = 0;
+
+    /**
+    *   Opens the file from a location on disk
+    *
+    *   Parameters identical to wxFile.
+    *
+    *   @param szFile file to open (full or relative path)
+    *   @param mode Mode to open the file in.  Only open with wxFile::read and wxFile::read_write
+    */
+    virtual bool Open(const char* szFile, OpenMode mode = wxFile::read);// = 0;
+    
+    /**
+    *   Reads data converted to PCM.  
+    *
+    *   @param nNumToRead number of UNCOMPRESSED bytes to read
+    *   @return number of UNCOMPRESSED bytes read
+    */
+    virtual off_t ReadPCM (void* pData, off_t nNumToRead) = 0;
+
+    /**
+    *   Seeks to a current time in the file
+    */
+    virtual double SeekTime(double dWhere) = 0;
+    
+////////////////////////////////////////////////////////////////////
+                            public:
+////////////////////////////////////////////////////////////////////
+    //
+    //  NORMAL FUNCTIONS
+    //
+
+    /**
+    *   Returns the number of bytes per second 
+    */
+    unsigned int GetBytesPerSecond() const;
+
+        
+    /**
+    *   Gets the number of bytes in each second (UNCOMPRESSED!).  
+    */
+    unsigned GetBytesPerSecondPCM() const;
+
+    /**
+    *   Degree of compression.  I.E. 3 means 1/3 the size.
+    */
+    double GetCompressionFactor() const;
+
+    /**
+    *   Gets the position in the playback of the file (in seconds)
+    */
+    double GetCurrentPosition() const;
+
+    /**
+    *   Gets the ending position of the currently playing file,
+    *   or when it last played.
+    */
+    double GetEndingPosition() const;
+
+    /**
+    *   Gets the format of the audio
+    */
+    int GetFormat() const;
+
+
+    /**
+    *   Obtains how long the file is in seconds
+    */
+    double GetPlayingTime() const; 
+
+    /**
+    *   Gets the starting position of the currently playing file,
+    *   or when it last played.
+    */
+    double GetStartingPosition() const;
+
+    /**
+    *   Gets the rate of the file (in khz).  Usually 44100 for wave files.
+    */
+    long GetRate() const;
+
+    /**
+    *   Returns true if the file is looping.
+    */
+    bool IsLooping() const;
+    
+    /**
+    *   Returns true if file or playing OR streaming.
+    */
+    bool IsPlaying();
+    
+    /**
+    *   Returns true if the file is playing & and is in stream mode
+    */
+    bool IsStreaming();
+
+    /**
+    *   Returns of number of UNCOMPRESSED bytes in this file
+    */
+    off_t LengthPCM() const;
+
+    /**
+    *   Plays the file.
+    *
+    *   @param dStartPos Number of seconds in the file to skip to
+    *   @param dEndPos Playing will stop (or loop) when this
+    *   @param nLoop How many times to loop playback of the file.
+    *   NOTE: Loops infinately
+    */
+    bool Play (double nStartPos = 0,
+               double nEndPos = -1,
+               bool bLoop = false,
+               bool bSeperateThread = true);
+
+    /**
+    *   Seeks to a point in the file (PCM version)
+    */
+    off_t SeekPCM(off_t where);
+
+
+    /**
+    *   Stops playing the file.
+    */
+    void Stop();
+
+    /**
+    *   Streams the file, buffering the playing rather than loading
+    *   the whole file beforehand.
+    */
+    bool Stream(
+               double nStartPos = 0,
+               double nEndPos = -1,
+               bool bLoop = false,
+               bool bSeperateThread = true);
+
+    //
+    //  Generally insane and dangerous to call SetSource and SetBuffers
+    //  while this file is playing :)
+    //
+    /**
+    *   Gets the source associated with this file.
+    */
+    const WXAUDIOSOURCE& GetSource();
+
+    /**
+    *   Gets the buffers associated with this file.
+    */
+    const WXAUDIOBUFFER*& GetBuffers();
+
+    /**
+    *   Sets the source associated with this file.
+    */
+    void SetSource(WXAUDIOSOURCE& kSource);
+
+    /**
+    *   Sets the buffers associated with this file.
+    */
+    void SetBuffers(WXAUDIOBUFFER* kBuffers);
+};//end of wxAudioFile
diff -ru2 -N -xCVS -x.#* -x*.orig -x*.rej -x*~ wxWindows.orig\include\wx\generic\wave.h wxWindows\include\wx\generic\wave.h
